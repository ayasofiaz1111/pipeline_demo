pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'building pip'
            }
        }
         stage('test') {
            steps {
                echo 'testing pip'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying'
            }
        }
         stage('release') {
            steps {
                echo 'Hello World'
            }
        }
    }
}
